pub fn echo(Table<number string> inputs) -> any {
    fn echo1(Table<number string> inputs) -> any {
        inputs[2] = "deep nesting"
        return inputs
    }

    return echo1(inputs)
}

string abcd = "abcd"
pub string public_abcd = "abcdPUBLIC"

Table<number string> output = echo({
    [0] = abcd,
    [1] = public_abcd
})

print(output[0], output[1], output[2])
